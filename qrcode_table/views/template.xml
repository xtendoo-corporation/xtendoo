                <div class="row mt-3 mb-3">
                    <div class="col-lg-9 product_main_block">
                        <div class="card h-100" style="width:100%!important;">
                            <div class="card-header">
                                <h3 class="text-center">
                                    <t t-esc="table_obj.name" />
                                </h3>
                            </div>
                             <div class="card-body">
                                    <div class="row">
                                        <div class="col-lg-3">
                                            <!-- Nav tabs -->
                                            <ul class="nav nav-pills">
                                                <t t-foreach="cate_ids" t-as="cat_id">
                                                    <li class="nav-item">
                                                        <a t-attf-href="#pos_table_category_#{cat_id.id}" t-attf-class="nav-link text-left pt-2 pb-2 #{'active' if cat_id.id == active_cat_id.id else ''}" data-toggle="tab">
                                                            <t t-esc="cat_id.name" />
