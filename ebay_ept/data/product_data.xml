<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ebay_product_categ_services" model="product.category">
        <field name="name">eBay Services</field>
    </record>
    <record id="product_product_ebay_shipment_fee" model="product.product">
        <field name="default_code">eBay SHIPPING FEE</field>
        <field name="list_price">0.0</field>
        <field name="standard_price">0.0</field>
        <field name="type">service</field>
        <field name="name">eBay Shipment Fee</field>
        <field name="description_sale">eBay Shipment Fee</field>
        <field name="invoice_policy">order</field>
        <field name="categ_id" ref="ebay_product_categ_services"/>
    </record>
    <record id="product_product_ebay_order_discount" model="product.product">
        <field name="default_code">ORDER DISCOUNT</field>
        <field name="list_price">0.0</field>
        <field name="standard_price">0.0</field>
        <field name="type">service</field>
        <field name="name">eBay Order Discount</field>
        <field name="description_sale">eBay Order Discount</field>
        <field name="invoice_policy">order</field>
        <field name="categ_id" ref="ebay_product_categ_services"/>
    </record>
</odoo>
