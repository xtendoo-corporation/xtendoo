<?xml version="1.0"?>
<odoo>
    <data>
        <record model="ir.cron" id="send_scheduled_notifications">
            <field name="name">Firebase: Send Scheduled Notifications</field>
            <field name="model_id" ref="model_firebase_notification"/>
            <field name="state">code</field>
            <field name="code">model.send_scheduled_notifications()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
