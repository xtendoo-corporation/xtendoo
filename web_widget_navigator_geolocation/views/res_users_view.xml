<?xml version="1.0" ?>
<odoo>

    <record id="inherited_res_users_form" model="ir.ui.view">
        <field name="name">inherited.res.users.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='signature']" position="after">
                <label for="navigator_geolocation" string="Navigator" class="oe_edit_only" />
                <h2>
                    <field name="is_geolocated" widget="navigator_geolocation_button"/>"/>
                </h2>
            </xpath>
        </field>
    </record>

</odoo>
